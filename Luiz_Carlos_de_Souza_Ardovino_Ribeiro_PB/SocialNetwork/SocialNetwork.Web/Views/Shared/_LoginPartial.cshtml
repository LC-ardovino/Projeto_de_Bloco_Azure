@using Microsoft.AspNet.Identity




@if (@HttpContext.Current.Session["access_token"] != null)
{
    using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm", @class = "navbar-right" }))
    {
        @Html.AntiForgeryToken()

<ul class="nav navbar-nav navbar-right">
    <li>@Html.ActionLink("Criar Perfil", "Create", "Profile", routeValues: null, htmlAttributes: new { id = "loginLink1" })</li>
    <li>@Html.ActionLink("Logoff", "Login", "Account", routeValues: null, htmlAttributes: new { id = "loginLink1" })</li>
</ul>
    }
}
else
{
    <ul class="nav navbar-nav navbar-right">
        <li>@Html.ActionLink("Registrar", "Register", "Account", routeValues: null, htmlAttributes: new { id = "registerLink" })</li>
        <li>@Html.ActionLink("Logar", "Login", "Account", routeValues: null, htmlAttributes: new { id = "loginLink" })</li>
    </ul>
}
